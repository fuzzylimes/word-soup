{
    "name": "Word Soup",
    "version": "0.1",
    "description": "An extension that does nothing!",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "html/popup.html",
        "default_icon": {
            "128": "img/large.jpg"
        }
    },
    "icons": {
        "128": "img/large.jpg"
    },
    "options_page": "html/options.html",
    "manifest_version": 2
}